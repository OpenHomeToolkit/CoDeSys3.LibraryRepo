ender.AINFO := ADR(alarmPayload);&#xD;&#xA;        sender();&#xD;&#xA;        &#xD;&#xA;        IF(sender.DONE OR sender.ERROR) THEN		&#xD;&#xA;            sender(REQ := FALSE);&#xD;&#xA;        END_IF&#xD;&#xA; " ObjectGUID="{f6433d9b-94db-403f-bb14-d8000354f11a}">
